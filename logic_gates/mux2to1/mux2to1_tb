v0x55ff09159ff0_0 .net "y", 0 0, L_0x55ff0915a4c0;  alias, 1 drivers
L_0x55ff0915a4c0 .functor MUXZ 1, v0x55ff0915a160_0, v0x55ff0915a220_0, v0x55ff0915a2f0_0, C4<>;
    .scope S_0x55ff09147dd0;
T_0 ;
    %vpi_call 2 9 "$monitor", "Time = %0t | a = %b | b = %b | sel = %b | y = %b", $time, v0x55ff0915a160_0, v0x55ff0915a220_0, v0x55ff0915a2f0_0, v0x55ff0915a3f0_0 {0 0 0};
    %vpi_call 2 10 "$dumpfile", "mux2to1.vcd" {0 0 0};
    %vpi_call 2 11 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x55ff09147dd0 {0 0 0};
